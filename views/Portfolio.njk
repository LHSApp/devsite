
{% extends "layout.njk"%}

 
    {%  block head %}
<title>Sobre - {{about.name}}</title>
    {% endblock %}

  
{% block content%}
    <div id="wrapper">
      <img
        src="{{about.avatar_url}}"
        alt="{{about.name}}"
      />
      <h1>{{about.name}}</h1>

      <h2>{{about.role}}</h2>
    </div>

    <div id="links-footer" class="links">

      {%for link in about.link%}
      <a href="{{link.url}}" target="_blank" rel="noopener">{{link.name}}</a>

      {% endfor %}
    </div>

    {% endblock%}


